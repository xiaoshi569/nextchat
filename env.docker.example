# Docker Compose 环境变量配置
# 复制此文件为 .env 并填写实际值

# 数据库密码
DB_PASSWORD=nextchat_secure_password_change_me

# JWT密钥（生产环境必须修改！）
JWT_SECRET=your-super-secret-jwt-key-please-change-in-production

# 加密密钥（至少32字符，生产环境必须修改！）
ENCRYPTION_KEY=your-encryption-key-32-characters-minimum-required

# 是否允许用户注册
ALLOW_REGISTER=true

# AI API 配置（可选，也可以在管理后台配置）
OPENAI_API_KEY=
GOOGLE_API_KEY=
BASE_URL=
CODE=

# 其他配置
OPENAI_ORG_ID=
HIDE_USER_API_KEY=
DISABLE_GPT4=
ENABLE_BALANCE_QUERY=
DISABLE_FAST_LINK=
OPENAI_SB=
PROXY_URL=

